{
  "manifest_version": 2,

  "name": "Terence Music",
  "description": "Terence Music App",
  "version": "1.0",
  "options_page": "index.html",
  "content_security_policy": "script-src 'self' http://localhost:* https://*.firebaseio.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://www.youtube.com/",
    "https://*.googlevideo.com/",
    "http://localhost:*/"
  ]
}
